    "@aws-cdk/aws-eventschemas": "1.130.0",
    "@aws-cdk/aws-iam": "1.130.0",
    "@aws-cdk/aws-lambda": "1.130.0",
    "@aws-cdk/aws-lambda-event-sources": "1.130.0",
    "@aws-cdk/aws-lambda-nodejs": "1.130.0",
    "@aws-cdk/aws-rds": "1.130.0",
