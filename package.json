{
  "name": "truman",
  "version": "1.0.012",
  "private": true,
  "scripts": {
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Use yarn for installing: https://yarnpkg.com/en/docs/install')\"",
    "compile": "tsc",
    "build": "./scripts/build.sh",
    "cleanup": "rm -Rf build",
    "eslint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache --fix",
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --list-different src/",
    "prettier:write": "prettier --write src/",
    "typecheck": "concurrently --raw yarn:typecheck:*",
    "typecheck:services": "tsc --noEmit",
    "generate": "yarn run generate:prisma && yarn run generate:graphql-codegen",
    "generate:prisma": "prisma generate --schema src/libs/prisma/schema.prisma",
    "generate:graphql-codegen": "graphql-codegen",
    "prisma:migrate": "yarn prisma migrate dev --create-only --preview-feature --schema src/libs/prisma/schema.prisma",
    "prisma:migrate:deploy": "yarn prisma migrate deploy --schema src/libs/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema src/libs/prisma/schema.prisma",
    "test": "jest --maxWorkers=50%",
    "test:integration": "jest --maxWorkers=1 --config=./src/services/graphql/test/jest.integration.config.js",
    "cdk:synth": "bash ./scripts/cdk-synth.sh 352158704087 eu-west-1",
    "cdk:deploy:dev": "env-cmd -f .env.development bash ./scripts/cdk-deploy-to.sh 352158704087 eu-west-1 dev",
    "cdk:deploy:prod": "env-cmd -f .env.production bash ./scripts/cdk-deploy-to.sh 352158704087 eu-west-1 prod",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "^2.0.3",
    "@prisma/client": "2.30.2",
    "@sendgrid/client": "^7.2.2",
    "@sendgrid/helpers": "^7.2.0",
    "@sendgrid/mail": "^7.6.2",
    "@sentry/node": "^6.3.0",
    "@sentry/serverless": "^5.29.2",
    "@sentry/tracing": "^5.29.2",
    "archiver": "^5.3.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.826.0",
    "axios": "^0.22.0",
    "convict": "^6.2.1",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "global": "^4.4.0",
    "lint-staged": "^12.1.2",
    "micro": "^9.3.4",
    "nanoid": "^3.1.30",
    "node-fetch": "^2.6.6",
    "prisma": "2.30.2",
    "slugify": "^1.6.3",
    "snake-case": "^3.0.4",
    "snakecase-keys": "^5.1.2",
    "source-map-support": "^0.5.16",
    "stream": "^0.0.2",
    "stripe": "^8.144.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.130.0",
    "@aws-cdk/aws-apigateway": "1.130.0",
    "@aws-cdk/aws-apigatewayv2": "1.130.0",
